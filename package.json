{
  "engines": {
    "node": "12.18.4"
  },
  "name": "dom-corleone-bot-discord",
  "version": "1.0.0",
  "description": "commander bot for the safezone discord server",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev src/server.ts",
    "start": "set NODE_ENV=production && npm cache clean --force && node dist/server.js",
    "format": "prettier --write ./src/**/*.{js,jsx,ts,tsx,json,md}",
    "lint": "eslint ./src/**/*.{ts,js}",
    "postinstall": "npm run build",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "author": "Marcelo Araujo",
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "^0.6.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "discord.js": "^12.5.1",
    "dotenv": "^10.0.0",
    "enmap": "^5.8.2",
    "express": "^4.17.1",
    "ffmpeg": "^0.0.4",
    "ffmpeg-static": "^4.4.0",
    "fluent-ffmpeg": "^2.1.2",
    "giphy-random": "^2.0.8",
    "heroku": "^7.47.3",
    "install": "^0.13.0",
    "pg": "^8.6.0",
    "pornsearch": "^2.4.3",
    "postgres": "^1.0.2",
    "standard": "^16.0.0",
    "typeorm": "^0.2.32",
    "ytdl-core": "^4.9.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2"
  },
  "keywords": []
}
