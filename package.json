{
  "name": "dom-corleone-bot-discord",
  "version": "1.0.0",
  "description": "commander bot for the safezone discord server",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --ignore-watch node_modules --no-notify src/server.ts",
    "start": "set NODE_ENV=production && yarn cache clean --force && node dist/server.js",
    "postinstall": "tsc",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "author": "Marcelo Araujo",
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "^0.5.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "enmap": "^5.8.2",
    "express": "^4.17.1",
    "giphy-random": "^2.0.8",
    "heroku": "^7.47.3",
    "pg": "^8.5.1",
    "pornsearch": "^2.4.3",
    "standard": "^16.0.0",
    "typeorm": "^0.2.29",
    "ytdl-core": "^4.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.2"
  },
  "engines": {
    "node": "12.18.4"
  },
  "keywords": []
}
